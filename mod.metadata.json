{
    "metadata_version": 1,

    "name": "Example Mod",
    "version": "1.0.0",
    "id": "examplemod",
    "type": "mod",

    "license": {
        "display_name": "LGPL-3.0",
        "url": "https://www.gnu.org/licenses/lgpl-3.0.en.html"
    },

    "contributors": [
        {
            "name": "Example Person",
            "role": "Owner"
        }
    ],

    "links": {
        "home": "",
        "source": "",
        "issues": ""
    },

    "loader": {
        "environment": "client",
        "access_wideners": [
            "examplemod.accesswidener"
        ],
        "mixins": [
            "examplemod.mixins.json"
        ],
        "entrypoints": [
            {
                "type": "common",
                "value": "com.example.ExampleMod::initialize"
            },
            {
                "type": "client",
                "value": "com.example.ExampleModClient"
            },
            {
                "type": "server",
                "adapter": "groovy",
                "value": "com.example.ExampleModServer"
            }
        ],

        "dependencies": [
            {
                "id": "minecraft",
                "version": "~1.19"
            },
            {
                "id": "other-example-mod",
                "version": "1.0.0",
                "unless": "other-other-example-mod"
            },
            {
                "id": "test-mod",
                "version": "1.0.0",
                "only-if": "other-example-mod"
            }
        ]
    },

    "additional": {
        "example": "Hello!"
    }
}
